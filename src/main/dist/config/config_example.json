{
	"debug": false,

	"loginToken": "",

	"ownerId": "",

	// Users with any of these roles will have additional permission.
	"permissionRoleId": [
		""
	],

	// One channel per server. The bot will listen to commands in only that channel.
	"channelId": [
		""
	],

	// Prefix for commands, spaces are not supported.
