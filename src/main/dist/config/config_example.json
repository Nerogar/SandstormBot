	"fileExtensions": [
		"aac",
		"flac",
		"ape",
		"m4a",
		"wmv",
		"wav",
		"m4v",
		"mp4",
		"mp3",
		"mpeg"
	],

	// To generate an api key:
	// 1. visit reddit.com/prefs/apps
	// 2. create a new app
	// 3. fill out the required fields, select the script app type
	"redditClientId": null,
	"redditClientSecret": null,

	// external program commands
	"ffmpegCommand" : "ffmpeg",
	"youtubDlCommand" : "youtube-dl"

}
